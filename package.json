{
	"name": "taro-unit-test",
	"version": "1.0.0",
	"private": true,
	"description": "Unit test sample for Taro framework",
	"templateInfo": {
		"name": "default",
		"typescript": true,
		"css": "sass"
	},
	"scripts": {
		"build:weapp": "taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"test": "jest",
		"test:ci": "taro build --type h5 --env development && npm run test"
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@tarojs/components": "1.3.35",
		"@tarojs/router": "1.3.35",
		"@tarojs/taro": "1.3.35",
		"@tarojs/taro-alipay": "1.3.35",
		"@tarojs/taro-h5": "1.3.35",
		"@tarojs/taro-swan": "1.3.35",
		"@tarojs/taro-tt": "1.3.35",
		"@tarojs/taro-weapp": "1.3.35",
		"nervjs": "^1.5.6",
		"nerv-devtools": "^1.5.6"
	},
	"devDependencies": {
		"@types/react": "^16.4.6",
		"@types/webpack-env": "^1.13.6",
		"@tarojs/cli": "1.3.35",
		"@tarojs/plugin-babel": "1.3.35",
		"@tarojs/plugin-csso": "1.3.35",
		"@tarojs/plugin-sass": "1.3.35",
		"@tarojs/plugin-uglifyjs": "1.3.35",
		"@tarojs/webpack-runner": "1.3.35",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-jsx-stylesheet": "^0.6.5",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-eslint": "^8.2.3",
		"eslint": "^5.16.0",
		"eslint-config-taro": "1.3.35",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-import": "^2.12.0",
		"stylelint": "9.3.0",
		"stylelint-config-taro-rn": "1.3.35",
		"stylelint-taro-rn": "1.3.35",
		"eslint-plugin-taro": "1.3.35",
		"@typescript-eslint/parser": "^1.6.0",
		"nerv-test-utils": "^1.5.6",
		"nerv-is-commonjs": "^1.4.1",
		"jest": "24.6.0",
		"@types/jest": "^24.0.12",
		"babel-jest": "^23.6.0",
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.1.0",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-transform-react-jsx": "^7.0.0",
		"typescript": "^3.0.1"
	}
}
